{
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": "warn",
        "eol-last": "error",
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "no-trailing-spaces": "error",
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}
